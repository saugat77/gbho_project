{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "artesaos/seotools": "^0.20.1",
        "barryvdh/laravel-dompdf": "^0.8.7",
        "biscolab/laravel-recaptcha": "^4.4",
        "bumbummen99/shoppingcart": "^4.0",
        "consoletvs/charts": "7.*",
        "diglactic/laravel-breadcrumbs": "^6.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "goldspecdigital/laravel-eloquent-uuid": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "infureal/artisan-gui": "^2.0",
        "intervention/image": "^2.5",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^8.0",
        "laravel/legacy-factories": "^1.1",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/omnipay": "^3.2",
        "livewire/livewire": "^2.2",
        "omnipay/paypal": "^3.0",
        "qcod/laravel-settings": "^1.0.9",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "spatie/laravel-permission": "^3.17",
        "spatie/laravel-sluggable": "^2.0",
        "sqits/laravel-userstamps": "^0.0.8",
        "srmklive/paypal": "~3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
